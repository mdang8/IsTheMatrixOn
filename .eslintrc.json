{
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "func-names": ["error", "never"],
    "prefer-arrow-callback": ["error", { "allowUnboundThis": true }]
  }
}

